@page "/learnRouting"
@page "/newRoutingPath"
@page "/learnRouting/{parameter1}"
@page "/learnRouting/{parameter1}/{parameter2}"

<h3>LearnRoutingComponent</h3>

<p class="text-danger">
    Parameter1: @Parameter1
</p>
<p class="text-secondary">
    Parameter2: @Parameter2
</p>

<NavLink class="btn btn-primary" href="learnAdvancedRouting?Param1=Blazzor&Param2=Blazzzor">
    <span class="oi oi-home" aria-hidden="true"></span> Routing with query param
</NavLink>

@code {
    [Parameter]
    public string Parameter1 { get; set; }

    [Parameter]
    public string Parameter2 { get; set; }

}
